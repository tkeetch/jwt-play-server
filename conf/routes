GET     /assets/*file                controllers.Assets.at(path="/public", file) 

GET     /login                       uk.co.tkeetch.sso.controllers.Auth.getLoginPage()

OPTIONS /login.json                  uk.co.tkeetch.CorsController.okAllowAll()
POST    /login.json                  uk.co.tkeetch.sso.controllers.Auth.doLoginJson()

OPTIONS /refresh.json                uk.co.tkeetch.CorsController.okAllowAll()
POST    /refresh.json                uk.co.tkeetch.sso.controllers.Auth.doRefreshJson()

GET     /                            uk.co.tkeetch.sso.controllers.Auth.redirectToLoginPage() 
